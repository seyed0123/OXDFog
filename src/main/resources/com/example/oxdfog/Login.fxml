<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oxdfog.Login">
   <children>
      <ImageView fitHeight="400.0" fitWidth="610.0" layoutX="-5.0" pickOnBounds="true">
         <image>
            <Image url="@OXDFog.jpeg" />
         </image>
      </ImageView>
      <Button fx:id="loginButton" layoutX="268.0" layoutY="238.0" mnemonicParsing="false" onAction="#login" text="login" />
      <TextField fx:id="idBar" layoutX="219.0" layoutY="125.0" prefHeight="18.0" prefWidth="149.0" promptText="id" />
      <PasswordField fx:id="passwordBar" layoutX="219.0" layoutY="165.0" promptText="password" />
      <Button fx:id="signUpButton" layoutX="261.0" layoutY="276.0" mnemonicParsing="false" onAction="#signUp" text="sign up" />
      <Label alignment="CENTER" layoutX="126.0" layoutY="21.0" prefHeight="75.0" prefWidth="326.0" text="OXDFog" textFill="#ff9500">
         <font>
            <Font name="Britannic Bold" size="56.0" />
         </font>
      </Label>
      <Label fx:id="loginStatus" alignment="CENTER" layoutX="189.0" layoutY="200.0" prefHeight="30.0" prefWidth="198.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
